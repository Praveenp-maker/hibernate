 <hibernate-mapping>  
  <class name="com.hibernate.Owner" table="owner">  
    <id name="oid">  
     <generator class="assigned"></generator>  
    </id>  
            
    <property name="name"></property>  
   
      <property name="phoneno"></property>  
   
            
  </class>  <class name="com.hibernate.Shop" table="shop">  
